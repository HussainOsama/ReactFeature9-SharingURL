<Home />\n        </Route>\n      </Switch>\n    </ThemeProvider>\n  );\n}\n\nexport default App;\n","/Users/Hussain/Development/ReactFeature9-SharingURL/src/styles.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/products.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/components/Home.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/components/NavBar.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/components/ProductList.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/components/ProductDetail.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/components/ProductItem.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/components/SearchBar.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/components/buttons/DeleteButton.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/stores/productStore.js",["64","65"],"import products from \"../products\";\nimport { makeObservable, observable, action } from \"mobx\";\nimport slugify from \"react-slugify\";\nimport axios from \"axios\";\n\nclass ProductStore {\n  products = [];\n  constructor() {\n    makeObservable(this, {\n      products: observable,\n      deleteProduct: action,\n      createProduct: action,\n    });\n  }\n\n  //Works\n  fetchProducts = async () => {\n    try {\n      const res = await axios.get(\"http://localhost:8000/products\");\n      this.products = res.data;\n    } catch {\n      console.error(\"404 Page Not Found\");\n    }\n  };\n\n  //Works\n  deleteProduct = async (productId) => {\n    try {\n      axios.delete(`http://localhost:8000/products/${productId}`);\n      this.products = this.products.filter(\n        (product) => productId !== product.id\n      );\n    } catch (error) {\n      console.log(\"CookieStore -> deleteCookie -> error\", error);\n    }\n  };\n\n  createProduct = async (product) => {\n    // product.id = products[products.length - 1].id + 1;\n    // product.slug = slugify(product.name);\n    // this.products.push(product);\n\n    try {\n      console.log(product);\n      const res = await axios.post(\"http://localhost:8000/products\", product);\n      this.products.push(res.data);\n    } catch (error) {\n      console.log(\"CookieStore -> createCookie -> error\", error);\n    }\n  };\n\n  // Still need Update function\n}\n\nlet productStore = new ProductStore();\nproductStore.fetchProducts();\n\nexport default productStore;\n","/Users/Hussain/Development/ReactFeature9-SharingURL/src/components/buttons/AddButton.js",[],"/Users/Hussain/Development/ReactFeature9-SharingURL/src/components/modals/ProductModal.js",[],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":32,"column":10,"nodeType":"72","messageId":"73","endLine":32,"endColumn":17},{"ruleId":"70","severity":1,"message":"74","line":1,"column":8,"nodeType":"72","messageId":"73","endLine":1,"endColumn":16},{"ruleId":"70","severity":1,"message":"75","line":3,"column":8,"nodeType":"72","messageId":"73","endLine":3,"endColumn":15},"no-native-reassign",["76"],"no-negated-in-lhs",["77"],"no-unused-vars","'product' is assigned a value but never used.","Identifier","unusedVar","'products' is defined but never used.","'slugify' is defined but never used.","no-global-assign","no-unsafe-negation"]